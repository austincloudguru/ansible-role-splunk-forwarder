[default]
index = {{ splunk_forwarder_index }}

blacklist = {{ splunk_forwarder_input_blacklist }}
ignoreOlderThan = 7d
sourcetype = {{ splunk_forwarder_sourcetype }}

{% for input_src in splunk_forwarder_logs %}
[monitor://{{ input_src }}]
{% endfor %}
